{
  "name": "react-context-mcp",
  "version": "0.2.1",
  "description": "MCP server for React DevTools - inspect React applications via Model Context Protocol",
  "type": "module",
  "bin": {
    "react-context-mcp": "build/src/main.js"
  },
  "main": "./build/src/main.js",
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('build', {recursive: true, force: true})\"",
    "build": "tsc",
    "start": "npm run build && node build/src/main.js",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "test:example": "npm run build && npx tsx examples/integration-test.ts"
  },
  "files": [
    "build/src",
    "LICENSE",
    "README.md",
    "!*.tsbuildinfo"
  ],
  "keywords": [
    "mcp",
    "model-context-protocol",
    "react",
    "react-devtools",
    "debugging",
    "chrome-devtools",
    "puppeteer",
    "ai",
    "llm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uxfreak/React-Devtools-MCP.git"
  },
  "bugs": {
    "url": "https://github.com/uxfreak/React-Devtools-MCP/issues"
  },
  "homepage": "https://github.com/uxfreak/React-Devtools-MCP#readme",
  "author": "uxfreak",
  "license": "Apache-2.0",
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || >=23"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "1.21.1",
    "core-js": "3.47.0",
    "debug": "4.4.3",
    "puppeteer": "24.31.0",
    "puppeteer-core": "24.31.0",
    "react-devtools-core": "^7.0.1",
    "yargs": "18.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^24.3.3",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.9.2"
  }
}
